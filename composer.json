{
    "name": "n5s/wpstarter",
    "type": "wpstarter-extension",
    "authors": [
        {
            "name": "Nicolas Lemoine",
            "email": "nico@niconico.fr"
        }
    ],
    "require": {
        "wecodemore/wpstarter": "3.0.0-beta.13",
        "roots/wordpress": "^6.1"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^12"
    },
    "extra": {
        "wpstarter-autoload": {
            "psr-4": {
                "n5s\\WpStarter\\": "src/"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true,
            "composer/installers": true,
            "wecodemore/wpstarter": true
        }
    },
    "scripts": {
        "cs": "ecs check",
        "cs:fix": "ecs check --fix"
    }
}
